{
  "manifest_version": 2,
  "name": "Touch Screen",
  "description": "Link to Google Apps Certification website",
  "version": "1.1",
  "icons": {
    "128": "tile.png"
  },
  "app": {
    "urls": [
      "http://certification.googleapps.com/app-info/"
    ],
    "launch": {
      "web_url": "http://certification.googleapps.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
